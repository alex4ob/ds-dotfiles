Host *
   ServerAliveInterval 10
   ServerAliveCountMax 2
   ForwardAgent yes
   User ${username}
   IdentityFile ~/.ssh/id_rsa
 
Host dev
     HostName dev-${username}.own-backup-dev.com
     User ${username}
     IdentityFile ~/.ssh/id_rsa

Host gitlab
     HostName git.own-backup.com
     User ${username}
     IdentityFile ~/.ssh/id_rsa